{
  "base_keymap": "VSCode",
  "buffer_font_size": 13,
  "cursor_blink": false,
  "inlay_hints": { "enabled": true },
  "scrollbar": { "show": "never" },
  "terminal": { "blinking": "on" },
  "ui_font_family": "JetBrains Mono",
  "ui_font_size": 13,
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "dark": "Gruvbox Dark",
    "light": "Gruvbox Dark"
  },
  "languages": {
    "TypeScript": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    },
    "TSX": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    }
  }
}
