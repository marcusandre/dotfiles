[user]
  name = Marcus André
  email = hello@marcusandre.de
[alias]
  l = log --pretty=oneline --abbrev-commit --graph --decorate
  join = merge --no-ff
  track = checkout -t
  unstage = checkout -- .
  amend = commit --amend
  today = log --since=midnight --author='Marcus André' --oneline --no-merges
  today-fs = log --since=midnight --no-merges --name-only
  ack = log --color=always --all --topo-order --pretty='format:%Cgreen%h%Creset %s%Cred%d%Creset %C(black bold)(by %an)%Creset' --full-history -S
[core]
  excludesfile = /Users/marcus/.gitignore_global
[color]
  ui = true
[credential]
  helper = osxkeychain
[github]
  user = marcusandre
[push]
  default = simple
[help]
  autocorrect = 1
